    "# Create new file or folder \n",
    "New-Item \"File name\"\n",
    "\n",
    "# Jump into folder \n",
    "cd path\n",
    "\n",
    "# Jump outside folder \n",
    "cd ..\n",
    "\n",
    "# Rename item or folder \n",
    "Rename-Item -Path \"OldName.txt\" -NewName \"NewName.txt\"\n",
    "\n",
    "# Delete item or folder\n",
    "Remove-Item \"Name\"\n",
    "\n"
   ]
  }
